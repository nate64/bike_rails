<h1>New Part</h1>
<%= form_for @parts do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :color %>
  <%= f.text_field :color %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :manufacturer %>
  <%= f.text_field :manufacturer %>

  <%= f.submit %>
<% end %>


